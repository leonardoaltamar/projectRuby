<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">

<p id="notice"><%= notice %></p>

<h1>Multa</h1>

<table class="table">
  <thead>
    <tr>
      <th>Valor</th>
      <th>Fecha</th>
      <th>Estado</th>
      <th>Prestamo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @multa.each do |multum| %>
      <tr>
        <td><%= multum.valor %></td>
        <td><%= multum.fecha %></td>
        <td><%= multum.estado %></td>
        <td><%= multum.Prestamo.Usuario.nombres %></td>
        <td><%= link_to 'Show', multum %></td>
        <td><%= link_to 'Edit', edit_multum_path(multum) %></td>
        <td><%= link_to 'Destroy', multum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Multum', new_multum_path %>
